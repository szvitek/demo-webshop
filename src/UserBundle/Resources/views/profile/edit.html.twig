{% extends '::layout.html.twig' %}

{% block content %}
    <h1>Edit User Information</h1>
    {{ form_start(form) }}
        {{ form_row(form.fullName) }}
        {{ form_row(form.address) }}
        {{ form_row(form.city) }}
        {{ form_row(form.county) }}
        {{ form_row(form.zip) }}
        {{ form_row(form.country) }}
        <button type="submit">Edit!</button>
    {{ form_end(form) }}
{% endblock %}